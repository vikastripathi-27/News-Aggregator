<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.journaldev.androidrssfeedtutorial">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Swipe_act_health"></activity>
        <activity android:name=".Swipe_act_business" />
        <activity android:name=".Swipe_act_entertainement" />
        <activity android:name=".Swipe_act_tech" />
        <activity android:name=".DetailActivity" />
        <activity android:name=".Swipe_act" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BrowserActivity" />
        <activity android:name=".RSSFeedActivity" />
    </application>

</manifest>